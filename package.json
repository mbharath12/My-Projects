{
  "name": "canopy-tests",
  "version": "1.0.0",
  "description": "Suite of tests to ensure systematic stability of Canopy Servicing",
  "main": "index.js",
  "private": true,
  "scripts": {
    "cy:open": "./node_modules/.bin/cypress open",
    "cy:run": "./node_modules/.bin/cypress run",
    "cy:runSmokeSpec": "./node_modules/.bin/cypress run --spec cypress/api/integration/core_api/**/*.*",
    "cy:smoketags": "./node_modules/.bin/cypress run --env tags='smoke'",
    "cy:regression": "./node_modules/.bin/cypress run --env tags='regression'",
    "cy:multipletags": "./node_modules/.bin/cypress run --env tags='creditOffSet;debitOffSet;manualFee'",
    "cy:rc": "./node_modules/.bin/cypress open --env fileConfig=rc",
    "watch": "run-p lint 'test --verbose --watchAll --runInBand'",
    "eslint": "eslint --ext js,ts,tsx",
    "prettier": "prettier ./ --write",
    "lint": "npm run eslint -- cypress",
    "tsc": "tsc"
  },
  "keywords": [
    "testing"
  ],
  "author": "Canopy Servicing",
  "license": "UNLICENSED",
  "devDependencies": {
    "@babel/core": "7.15.0",
    "@babel/eslint-parser": "^7.14.3",
    "@babel/preset-env": "7.15.0",
    "@babel/preset-typescript": "7.15.0",
    "@canopyinc/api-docs": "1.6.2",
    "@cypress/webpack-preprocessor": "^5.9.1",
    "@types/node": "^16.7.1",
    "@types/uuid": "^8.3.1",
    "@typescript-eslint/eslint-plugin": "4.29.1",
    "@typescript-eslint/parser": "4.29.1",
    "babel-loader": "8.2.2",
    "csvtojson": "^2.0.10",
    "dotenv": "^10.0.0",
    "eslint": "^7.27.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-cypress": "^2.11.3",
    "eslint-plugin-import": "^2.23.4",
    "husky": "4.3.0",
    "lint-staged": "10.0.8",
    "prettier": "2.2.1",
    "pretty-quick": "3.0.2",
    "typescript": "^4.3.4"
  },
  "engines": {
    "node": ">=14"
  },
  "dependencies": {
    "@types/k6": "^0.35.2",
    "ajv": "8.6.2",
    "ajv-formats": "2.1.1",
    "ajv-i18n": "4.1.0",
    "cypress": "^8.2.0",
    "cypress-promise": "^1.1.0",
    "cypress-xpath": "^1.6.2",
    "eslint-plugin-tsdoc": "^0.2.14",
    "fs-extra": "^10.0.0",
    "moment-timezone": "^0.5.34",
    "pg": "^8.7.1",
    "pg-native": "^3.0.0",
    "soft-assert": "^0.2.6",
    "uuid": "^8.3.2",
    "webpack": "^5.52.1"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "prettier --write",
      "npm run eslint -- --fix"
    ],
    "*.{md,json}": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && lint-staged"
    }
  }
}
